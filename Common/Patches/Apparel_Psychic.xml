<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<!-- Royalty\Defs\ThingDefs_Misc\Apparel_Psychic.xml -->

	<!-- Apparel_PsyfocusHelmet -->

	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName="Apparel_PsyfocusHelmet"]</xpath>
		<value>
			<recipeMaker>
				<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
				<workSkill>Crafting</workSkill>
				<effectWorking>Smith</effectWorking>
				<soundWorking>Recipe_Tailor</soundWorking>
				<recipeUsers>
					<li>FabricationBench</li>
				</recipeUsers>
				<unfinishedThingDef>UnfinishedEltexApparel</unfinishedThingDef>
				<researchPrerequisite>EltexApparel</researchPrerequisite>
				<skillRequirements>
					<Crafting>16</Crafting>
				</skillRequirements>
			</recipeMaker>
			<costList>
				<Cloth>20</Cloth>
				<Hyperweave>5</Hyperweave>
				<Plasteel>10</Plasteel>
				<Silver>3</Silver>
				<Gold>1</Gold>
				<ComponentSpacer>2</ComponentSpacer>
			</costList>
		</value>
	</Operation>

	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName="Apparel_PsyfocusHelmet"]/statBases</xpath>
		<value>
			<WorkToMake>40000</WorkToMake>
		</value>
	</Operation>

	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ThingDef[defName="Apparel_PsyfocusHelmet"]/statBases/MarketValue</xpath>
		<value>
			<MarketValue>1000</MarketValue>
		</value>
	</Operation>

