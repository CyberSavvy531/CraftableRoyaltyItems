<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <!-- Core\Defs\RecipeDefs\Recipes_Production.xml -->

    <!-- Apparel_PsyfocusHelmet -->

    <RecipeDef>
        <defName>Make_Apparel_PsyfocusHelmet</defName>
        <label>Make eltex helmet</label>
        <description>Make an eltex helmet.</description>
        <jobString>Making eltex helmet.</jobString>
        <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
        <effectWorking>Smith</effectWorking>
        <soundWorking>Recipe_Smith</soundWorking>
        <workAmount>40000</workAmount>
        <unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>ComponentSpacer</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Cloth</li>
                    </thingDefs>
                </filter>
                <count>20</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Plasteel</li>
                    </thingDefs>
                </filter>
                <count>10</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Gold</li>
                    </thingDefs>
                </filter>
                <count>0.3</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Silver</li>
                    </thingDefs>
                </filter>
                <count>2.5</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Hyperweave</li>
                    </thingDefs>
                </filter>
                <count>5</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>ComponentSpacer</li>
                <li>Cloth</li>
                <li>Plasteel</li>
                <li>Gold</li>
                <li>Silver</li>
                <li>Hyperweave</li>
                <li>Dye</li>
            </thingDefs>
        </fixedIngredientFilter>
        <products>
            <Apparel_PsyfocusHelmet>1</Apparel_PsyfocusHelmet>
        </products>
        <researchPrerequisite>EltexApparel</researchPrerequisite>
        <skillRequirements>
            <Crafting>16</Crafting>
        </skillRequirements>
        <workSkill>Crafting</workSkill>
    </RecipeDef>

